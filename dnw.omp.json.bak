{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#00897b",
          "foreground_templates": ["{{ if .Error }}#e91e63{{ end }}"],
          "template": " ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#15C2CB",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#F141A8",
          "template": "<#F3EFF5>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue0a0 "
          }
        },
        {
          "type": "project",
          "style": "plain",
          "foreground": "#5EADF2",
          "template": "{{if .Version}}<#F3EFF5>is</> \uf487 {{.Version}} {{end}}"
        },
        {
          "type": "react",
          "style": "plain",
          "foreground": "#61dafb",
          "template": "{{ if .Full }}<#F3EFF5>with</>  {{ .Full }} {{end}}"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#43853D",
          "template": "<#F3EFF5>via</> \ue718 {{ .Full }}{{ if .Env.TS }} <#F3EFF5>and</> <#3178C6>\ue628 {{ .Env.TS }}</>{{ end }} <#F3EFF5>and</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ .Env.MANAGER }}</>{{ end }} ",
          "properties": {
            "fetch_version": true,
            "fetch_package_manager": true,
            "display_mode": "context",
            "npm_icon": "<#FE4A49>  ",
            "yarn_icon": "<#117CAD>  "
          }
        },
        {
          "type": "go",
          "style": "plain",
          "template": "<#F3EFF5>with</> <#7FD5EA> {{ .Full }}</> "
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#9780e5",
          "template": "<#F3EFF5>via</>  {{ .Full }} "
        },
        {
          "type": "kotlin",
          "style": "plain",
          "foreground": "#906cff",
          "template": "<#F3EFF5>via</> <b>K</b> {{ .Full }} "
        }
      ]
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "#FFD664"
        },
        {
          "foreground": "#a9ffb4",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#a9ffb4",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s <#ffffff></>",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#94ffa2",
          "style": "diamond",
          "template": " <#ffffff></> {{ round .PhysicalPercentUsed .Precision }}% ",
          "type": "sysinfo",
          "properties": {
            "precision": 0
          }
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#a9ffb4",
          "template": "❯ "
        }
      ],
      "newline": true
    }
  ]
}
