{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\ue23a ",
          "foreground": "#00897b",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "{{ .Path }} ",
          "foreground": "#15C2CB",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "cache_duration": "none"
          },
          "template": "<#F3EFF5>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "#F141A8",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}<#F3EFF5>is</>  {{.Version}}{{ end }} {{ end }}",
          "foreground": "#5EADF2",
          "type": "project",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "{{ if .Full }}<#F3EFF5>with</> \ue7ba {{ .Full }} {{end}}",
          "foreground": "#61dafb",
          "type": "react",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h",
            "display_mode": "context",
            "fetch_package_manager": true,
            "fetch_version": true,
            "npm_icon": "<#FE4A49> \ue71e ",
            "yarn_icon": "<#117CAD> \ue6a7 "
          },
          "template": "<#F3EFF5>via</> \ue718 {{ .Full }}{{ if .Env.TS }} <#F3EFF5>and</> <#3178C6>\ue628 {{ .Env.TS }}</>{{ end }} <#F3EFF5>and</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ .Env.MANAGER }}</>{{ end }} ",
          "foreground": "#43853D",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "<#F3EFF5>with</> <#7FD5EA>\ue627 {{ .Full }}</> ",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "<#F3EFF5>via</> \ue77f {{ .Full }} ",
          "foreground": "#9780e5",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "<#F3EFF5>via</> <b>K</b> {{ .Full }} ",
          "foreground": "#906cff",
          "type": "kotlin",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#FFD664",
          "type": "upgrade",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#a9ffb4",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }}s <#ffffff></>",
          "foreground": "#a9ffb4",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7 ",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "precision": 0
          },
          "template": " <#ffffff>\ue266</> {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#94ffa2",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.Icon}} ",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "#a9ffb4",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
