{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\ue23a ",
          "foreground": "#00897b",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if .Error }}#e91e63{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "{{ .Path }} ",
          "foreground": "#15C2CB",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "cache_duration": "none",
            "mapped_branches": {
              "feature/*": "‚ú® feature/",
              "release/*": "üöÄ release/",
              "hotfix/*": "ü©π hotfix/",
              "bugfix/*": "üêõ bugfix/",
              "support/*": "ü©º support/",
              "dev": "üõ†Ô∏è dev",
              "master": "üóø master",
              "main": "üóø main"
            }
          },
          "template": "<#F3EFF5>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }} ",
          "foreground": "#F141A8",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}<#F3EFF5>is</> Ôíá {{.Version}}{{ end }}{{ end }}",
          "foreground": "#5EADF2",
          "type": "project",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "<#F3EFF5> with</> <#7FD5EA>\ue627 {{ .Full }}</>",
          "type": "go",
          "style": "plain"
        },
        {
          "type": "react",
          "style": "plain",
          "display_mode": "always",
          "foreground": "#61dafb",
          "template": "<#F3EFF5> with</> Óû∫ {{ .Full }}",
          "extensions": "package.json"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#6CA35E",
          "template": "<#F3EFF5> via</> Û∞éô {{ .Full }}"
        },
        {
          "template": "<#F3EFF5>via</> \ue77f {{ .Full }} ",
          "foreground": "#9780e5",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "style": "plain",
          "type": "kotlin",
          "foreground": "#906cff",
          "template": "<#F3EFF5>via</> <b>K</b> {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "plain",
          "foreground": "red",
          "template": " <#F3EFF5>and</> Óòñ {{ .Full }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }}s <#ffffff></>",
          "foreground": "#a9ffb4",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7 ",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "precision": 0
          },
          "template": " <#ffffff>\ue266</> {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#94ffa2",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.Icon}} ",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#a9ffb4",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
